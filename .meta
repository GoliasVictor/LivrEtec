  Nome: LivrEtec
  Tipo: Projeto
  Descricao: Projeto LivrEtec
  Linguagem: c#
  ComandoAbrir: code .
  SubProjetos:
    - Nome: LivrEtec
      Caminho: ./src/LivrEtec
    
    - Nome: GIB
      Caminho: ./src/LivrEtec.GIB
    
    - Nome: GIB.Servidor
      Caminho: ./src/LivrEtec.GIB.Servidor
    
    - Nome: Servidor
      Caminho: ./src/LivrEtec.Servidor
      Scripts:
        gib-up: dotnet run --project ./src/LivrEtec.GIB.Servidor
    
    - Nome: Testes
      Caminho: ./src/LivrEtec.Testes
      
  Scripts:
    test:        dotnet test ./src  --logger "console;verbosity=normal"
    test-remoto: dotnet test ./src --filter Category=Remoto  --logger "console;verbosity=normal"
    test-local:  dotnet test ./src --filter Category=local  --logger "console;verbosity=normal"
    gib:up:      dotnet run --project ./src/LivrEtec.GIB.Servidor
    gib:dev:      dotnet watch run --project ./src/LivrEtec.GIB.Servidor
    docker: sudo docker compose -f src/docker-compose.yml up --build --exit-code-from testes
    