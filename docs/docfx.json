{
  "metadata": [
    {
      "src": [ { "files": [ "LivrEtec/*.csproj" ], "src":"../src" } ],
      "dest": "api/LivrEtec",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [ { "files": [ "LivrEtec.Servidor/*.csproj" ], "src":"../src" } ],
      "dest": "api/LivrEtec.Servidor",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [ { "files": [ "LivrEtec.GIB/*.csproj" ], "src":"../src" } ],
      "dest": "api/LivrEtec.GIB",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [ { "files": [ "LivrEtec.GIB.Servidor/*.csproj" ], "src":"../src" } ],
      "dest": "api/LivrEtec.GIB.Servidor",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    },
    {
      "src": [ { "files": [ "LivrEtec.Testes/*.csproj" ], "src":"../src" } ],
      "dest": "api/LivrEtec.Testes",
      "disableGitFeatures": false,
      "disableDefaultFilter": false
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false
  }
}